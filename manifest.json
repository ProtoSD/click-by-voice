{
    "manifest_version": 2,

    "name": "Click by Voice",
    "description": "This Chrome browser extension is being developed to provide support for activating links and other HTML elements using voice commands.",
    "version": "0.0.1",

    "permissions": [
	"activeTab"
    ],


    "browser_action": {
	"default_title": "Click by Voice"
    },


    "content_scripts": [
	{
	    "matches": ["http://*/*", "https://*/*", "file://*/*"],
	    "css": ["show_hints.css"],
	    "js": ["jquery-3.0.0.min.js", "content_script.js"]
	}
    ]
}
